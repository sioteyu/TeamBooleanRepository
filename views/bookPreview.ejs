                      <ol class="comment-list">
                        <li class="comment even thread-even depth-1" id="li-comment-33" itemprop="review" itemscope itemtype="http://schema.org/Review" style="border:unset; margin-bottom:20px">
                          <div class="comment-wrapper" id="div-comment-33" style="background-color: white; padding:20px; border-radius: 4px;">
                            <header class="comment-header" style="z-index:0">
                              <div class="comment-author vcard" itemprop="author" itemscope itemtype="http://schema.org/Person">
                                <img src="images/bookPreview/profpic1.jpg" width="75" height="75" alt="" class="avatar avatar-75wp-user-avatar wp-user-avatar-75 alignnone photo avatar-default" /><span class="fn">John Doe</span>
                              </div><!-- .comment-author .vcard -->
