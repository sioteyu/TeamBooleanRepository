<h1>User List</h1>
<table class="table">
  <tr>
    <th>Email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
  </tr>
  <%for(var i = 0;i < data.length;++i){%>
  <tr>
    <td><%=data[i].email%></td>
    <td><%=data[i].firstname%></td>
    <td><%=data[i].lastname%></td>
    <td> <button type="button" class="btn btn-danger" name="button">Ban</button> </td>
  </tr>
  <%}%>
</table>
